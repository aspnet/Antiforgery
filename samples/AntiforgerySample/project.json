{
  "version": "1.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Antiforgery": "1.0.3",
    "Microsoft.AspNetCore.Http.Abstractions": "1.0.2",
    "Microsoft.AspNetCore.Http.Extensions": "1.0.2",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.2",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.3",
    "Microsoft.AspNetCore.StaticFiles": "1.0.2",
    "Newtonsoft.Json": "9.0.1"
  },
  "buildOptions": {
    "emitEntryPoint": true
  },
  "frameworks": {
    "net451": {},
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.3",
          "type": "platform"
        }
      }
    }
  },
  "publishOptions": {
    "include": [
      "wwwroot",
      "web.config"
    ]
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview4-update1"
  },
  "scripts": {
    "postpublish": "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%"
  }
}